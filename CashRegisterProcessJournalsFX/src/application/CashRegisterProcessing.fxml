<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9" fx:controller="cashRegisterController">
   <children>
      <ButtonBar layoutX="1.0" prefHeight="168.0" prefWidth="375.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#selectDirectory" text="Open Directory" />
            <Label fx:id="directoryLabel" prefHeight="17.0" prefWidth="277.0" text="Label" />
        </buttons>
      </ButtonBar>
      <ButtonBar layoutY="141.0" prefHeight="85.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="scanDirectory" mnemonicParsing="false" onAction="#scanDirectory" text="Scan Directory" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
